    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.2",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
