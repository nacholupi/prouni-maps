{
  "name": "prouni-maps",
  "version": "1.0.0",
  "scripts": {
    "start": "node backend/bin/www",
    "local": "ng build && node backend/bin/www",
    "local-front": "ng serve --proxy-config proxy.conf.json --disable-host-check",
    "local-back": "nodemon backend/bin/www",
    "build:prod": "ng build --configuration=production",
    "start:prod": "node backend/bin/www",
    "gdeploy": "ng build --configuration=production && gcloud app deploy",
    "extract-i18n": "ng xi18n --i18n-format xlf --output-path i18n --i18n-locale en && ng run crud:xliffmerge"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nacholupi/prouni-maps.git"
  },
  "author": "Ignacio Lupi",
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@angular/animations": "^7.1.3",
    "@angular/cdk": "^7.1.1",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "~7.1.0",
    "@angular/material": "^7.1.1",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@google/markerclusterer": "^1.0.3",
    "@ngx-i18nsupport/tooling": "^1.1.2",
    "@types/googlemaps": "^3.30.19",
    "body-parser": "^1.18.3",
    "classlist.js": "^1.1.20150312",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.4",
    "cors": "^2.8.5",
    "debug": "^4.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-prettify": "^0.1.1",
    "express-session": "^1.15.6",
    "hammerjs": "^2.0.8",
    "js-marker-clusterer": "^1.0.0",
    "mongoose": "^5.3.15",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "rxjs": "~6.3.3",
    "serve-favicon": "^2.5.0",
